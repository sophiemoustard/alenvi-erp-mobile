{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "noEmit": true,
    "jsx": "react-native",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "strict": true,
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types"]
  },
  "exclude": ["node_modules"],
  "extends": "expo/tsconfig.base"
}
