{
  "scripts": {
    "prestart": "source $HOME/.nvm/nvm.sh; nvm use 16.18.0",
    "start": "npx expo start",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "web": "npx expo start --web",
    "eject": "expo eject",
    "ts-lint": "tsc --noEmit && eslint environment.ts src/App/index.tsx ./src --ext .js,.jsx,.ts,.tsx -c .eslintrc.js",
    "eas-build-pre-install": "echo $ANDROID_GOOGLE_SERVICES_BASE64 | base64 --decode > ./google-services.json",
    "test": "node_modules/jest/bin/jest.js"
  },
  "jest": {
    "preset": "jest-expo",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|react-clone-referenced-element|@react-native-community|@react-native-async-storage|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ],
    "cacheDirectory": ".jest/cache",
    "setupFiles": [
      "./mock.js"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@formatjs/intl-datetimeformat": "^5.0.1",
    "@formatjs/intl-getcanonicallocales": "^1.9.2",
    "@formatjs/intl-locale": "^2.4.47",
    "@formatjs/intl-numberformat": "^7.4.3",
    "@formatjs/intl-pluralrules": "^4.3.3",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-community/datetimepicker": "6.1.2",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^5.11.10",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.4",
    "@types/luxon": "^2.0.8",
    "axios": "^0.27.2",
    "dotenv": "^16.3.1",
    "expo": "^45.0.0",
    "expo-app-loading": "~2.0.0",
    "expo-application": "~4.1.0",
    "expo-barcode-scanner": "~11.3.0",
    "expo-build-properties": "~0.2.0",
    "expo-camera": "~12.2.0",
    "expo-constants": "~13.1.1",
    "expo-dev-client": "~1.0.1",
    "expo-device": "~4.2.0",
    "expo-font": "~10.1.0",
    "expo-status-bar": "~1.3.0",
    "expo-system-ui": "~1.2.0",
    "expo-updates": "~0.13.4",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "luxon": "^2.3.1",
    "qs": "^6.11.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-gesture-handler": "~2.2.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "~3.11.1",
    "sentry-expo": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@testing-library/react-native": "^9.1.0",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.isequal": "^4.5.6",
    "@types/lodash.pick": "^4.4.7",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.48",
    "@types/react-dom": "^17.0.17",
    "@types/react-native": "^0.64.26",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "axios-mock-adapter": "^1.21.1",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^26.6.3",
    "jest-expo": "^44.0.0",
    "react-test-renderer": "^17.0.2",
    "sinon": "^13.0.2",
    "typescript": "~4.3.5"
  },
  "private": true
}
